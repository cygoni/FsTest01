<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.flashscoretest01">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".NewsActivity"></activity>
        <activity android:name=".RecordActivity" />
        <activity android:name=".ScheduleActivity" />
        <activity android:name=".Picture_RECActivity" />
        <activity android:name=".Rank2Activity" />
        <activity android:name=".RankActivity" />
        <activity android:name=".ToolbarActivity" />
        <activity
            android:name=".IntroActivity"
            android:theme="@style/AppTheme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTop" />
    </application>

</manifest>